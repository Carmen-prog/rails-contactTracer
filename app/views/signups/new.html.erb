<h1>Location Owners Sign-up Page</h2>

<%= form_for :signup, url: signups_path do |f| %>

  <p>
    <%= f.label :owner, "Full Legal Name *" %><br>
    <%= f.text_field :owner %>
  </p>
 
  <p>
    <%= f.label :shopname, "Shop Name *" %><br>
    <%= f.text_field :shopname %>
  </p>
 
   <p>
    <%= f.label :shoplocation, "Shop Location *" %><br>
    <%= f.text_field :shoplocation %>
  </p>

  <p>
    <%= f.submit %>
  </p>
  
<% end %>

<%= link_to 'Back', signups_path %>

